<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record id="lms_member_types_tree" model="ir.ui.view">
        <field name="name">lms.typemember.tree</field>
        <field name="model">lms.typemember</field>
        <field name="arch" type="xml">
            <tree>
                <field name='member_types'/>
                <field name='fees'/>
            </tree>
        </field>
    </record>


    <record id="lms_member_types_form" model="ir.ui.view">
        <field name="name">lms.typemember.form</field>
        <field name="model">lms.typemember</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name='member_types'/>
                            <field name='fees'/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    
</odoo>